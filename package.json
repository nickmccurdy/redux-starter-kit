{
  "name": "@acemarke/redux-starter-kit",
  "version": "0.0.1",
  "description": "A simple set of tools to make using Redux easier",
  "main": "dist/redux-starter-kit.cjs.js",
  "module": "dist/redux-starter-kit.esm.js",
  "browser": "dist/redux-starter-kit.umd.js",
  "author": "Mark Erikson <mark@isquaredsoftware.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.2",
    "babel-jest": "^23.4.2",
    "jest": "^23.5.0",
    "prettier": "^1.10.2",
    "rollup": "^0.56.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-typescript": "^0.8.1",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "format": "prettier --write \"**/*.{js,md}\"",
    "format:check": "prettier --list-different \"**/*.{js,md}\"",
    "prepare": "npm run format:check && npm run type && npm test && npm run build",
    "test": "jest",
    "type": "tsc --noEmit"
  },
  "repository": "markerikson/redux-starter-kit",
  "files": [
    "dist"
  ],
  "dependencies": {
    "immer": "^1.1.3",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "selectorator": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "sideEffects": false
}
