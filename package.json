{
  "name": "@acemarke/redux-starter-kit",
  "version": "0.0.7",
  "description": "A simple set of tools to make using Redux easier",
  "repository": "https://github.com/markerikson/redux-starter-kit",
  "main": "dist/redux-starter-kit.cjs.js",
  "module": "dist/redux-starter-kit.esm.js",
  "browser": "dist/redux-starter-kit.umd.js",
  "author": "Mark Erikson <mark@isquaredsoftware.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "eslint": "^4.17.0",
    "eslint-config-react-app": "^3.0.0-next.a671462c",
    "eslint-plugin-flowtype": "^2.44.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.4.2",
    "prettier": "^1.10.2",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "format": "prettier --write \"**/*.{js,ts,md}\"",
    "format:check": "prettier --list-different \"**/*.{js,ts,md}\"",
    "lint": "eslint --ext js,ts src",
    "prepare": "npm run format:check && npm run lint && npm run type && npm test && npm run build",
    "test": "jest",
    "type": "tsc"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "immer": "^1.1.3",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.3",
    "redux-thunk": "^2.2.0",
    "selectorator": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    }
  },
  "sideEffects": false
}
